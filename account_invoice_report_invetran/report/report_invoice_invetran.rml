    <story>
        <para style="terp_default_8">[[ repeatIn(objects,'o') ]]</para>
        <para style="terp_default_8">[[ setLang(o.partner_id.lang) ]]</para>

        <spacer length="1mm"/>
        <spacer length="27mm"/>
        <pto>
            <pto_header><!-- Must be after setLang() -->


            </pto_header>
            <spacer length="3cm"/>
            <blockTable colWidths="85,260.0,80.0,85.0" style="Table10">
                <tr>
                    <td>
                        <para style="titulot"><b>Cantidad</b></para>
                    </td>
                    <td>
                        <para style="titulot"><b>Descripción</b></para>
                    </td>
                    <td>
                        <para style="titulot"><b>Precio Unitario</b></para>
                    </td>
                    <td>
                        <para style="titulot"><b>Precio Total</b></para>
                    </td>
                </tr>
            </blockTable>

            <spacer length="0.5cm"/>
            <section>
                <para style="terp_default_2">[[ repeatIn(o.invoice_line,'l') ]]</para>

                <blockTable colWidths="85,240.0,80.0,85.0" style="Table8">
                    <tr>
                        <td>
                            <para style="terp_default_Centre_9">[[ formatLang(l.quantity,digits=2)]]</para>
                        </td>
                        <td>
                            <para style="terp_default_Centre_9">[[ format(l.name) ]]</para>
                        </td>
                        <td>
                            <para style="terp_default_Right_9">[[ formatLang(l.price_unit) ]] Bs</para>
                        </td>
                        <td>
                            <para style="terp_default_Right_9">[[ formatLang(l.price_subtotal) ]] Bs</para>
                        </td>
                    </tr>
                </blockTable>
            </section>
        </pto>
