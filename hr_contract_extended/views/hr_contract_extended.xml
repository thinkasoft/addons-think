<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="hr_contract_extended_form_view">
            <field name="name">view.hr.contract.extended</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form" />
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="Withholding">
                        <separator string="Data" colspan="4"/>
                        <group>
                            <group>
                                <group>
                                    <field name="income_wh"/>
                                    <field name="insurance_health_wh"/>
                                    <field name="saving_wh"/>
                                    <field name="nhs_wh"/>
                                    <field name="inces_wh"/>
                                </group>
                                <group>
                                    <field digits="(2, 2)" name="mod_income_wh"/>
                                    <field digits="(2, 2)" name="mod_insurance_health_wh"/>
                                    <field digits="(2, 2)" name="mod_saving_wh"/>
                                    <field name="lph_wh"/>
                                </group>
                            </group>
                        </group>
                    </page>
                </notebook>
            </field>
        </record>
    </data>
</openerp>
